{
	"compilerOptions":
	{
		"target": "ES2020",
		"module": "ESNext",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./build",
		"rootDir": "./src/ts"
	},
	"include": ["src/ts/main.ts"]
}
